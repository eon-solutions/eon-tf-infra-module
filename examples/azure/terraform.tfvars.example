# =============================================================================
# Eon Provider Configuration
# =============================================================================

# Eon API credentials - obtain from Eon console
eon_client_id     = "your-client-id"
eon_client_secret = "your-client-secret"
eon_endpoint      = "https://api.eon.io"
eon_project_id    = "your-project-id"

# =============================================================================
# Azure Account Configuration
# =============================================================================

# Azure tenant and subscription - obtain from Azure portal
tenant_id       = "12345678-1234-1234-1234-123456789012"
subscription_id = "12345678-1234-1234-1234-123456789012"

# Eon management app ID - provided by Eon
management_app_id = "12345678-1234-1234-1234-123456789012"

# Customer backup apps - created during Eon onboarding
backup_app_id         = "12345678-1234-1234-1234-123456789012"
backup_restore_app_id = "12345678-1234-1234-1234-123456789012"

# =============================================================================
# Optional Configuration
# =============================================================================

# Source account resource group location
source_resource_group_location = "East US"

# Restore account region
restore_location = "eastus"
